{
  "name": "imagey-web",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "prettier . --check && eslint src && eslint tests",
    "prettier": "prettier . -w",
    "preview": "vite preview",
    "test": "playwright test",
    "test:coverage": "nyc --reporter=lcov --reporter=text-summary playwright test",
    "test:coverage:report": "nyc --reporter=html report",
    "test:coverage:ui": "open coverage/index.html",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "i18n:check": "i18next 'src/**/*.ts*' -o 'src/translation/locales/$LOCALE/$NAMESPACE.json'"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "beercss": "^3.11.33",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "material-dynamic-colors": "^1.1.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.6.1",
    "react-router": "^7.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@pact-foundation/pact": "^15.0.1",
    "@playwright/test": "^1.47.2",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^15.9.0",
    "i18next-parser": "^9.3.0",
    "istanbul-lib-coverage": "^3.2.2",
    "istanbul-lib-report": "^3.0.1",
    "istanbul-reports": "^3.2.0",
    "nyc": "^17.1.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^6.2.0",
    "vite-plugin-istanbul": "^6.0.2"
  }
}
