    "@mdi/react": "^1.6.1",
    "@mui/material": "^6.1.2",
    "@pact-foundation/pact": "^13.1.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@playwright/test": "^1.47.2",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.11.1",
